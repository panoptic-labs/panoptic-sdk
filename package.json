{
  "name": "@panoptic-eng/sdk",
  "version": "1.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/panoptic-labs/panoptic-sdk.git"
  },
  "bugs": "https://github.com/panoptic-labs/panoptic-sdk/issues",
  "homepage": "https://github.com/panoptic-labs/panoptic-sdk#readme",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./test": {
      "types": "./dist/test/index.d.ts",
      "import": "./dist/test/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "(pnpm run codegen || echo 'codegen skipped (foundry projects not available)') && pnpm tsdown",
    "build:ci": "pnpm tsdown",
    "dev": "pnpm concurrently \"pnpm run codegen --watch\" \"pnpm tsdown --watch\"",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "lint": "eslint . --ext .ts,.tsx --ignore-pattern 'dist/**' --ignore-pattern 'graphql/**' --ignore-pattern 'src/graphql/**' --max-warnings=0",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest run",
    "test:fork": "vitest run --config src/panoptic/v2/examples/__tests__/vitest.config.fork.ts",
    "test:fork:watch": "vitest watch --config src/panoptic/v2/examples/__tests__/vitest.config.fork.ts",
    "test:examples": "pnpm test && pnpm test:fork",
    "codegen": "pnpm run codegen:graphql && pnpm run codegen:wagmi",
    "codegen:graphql": "graphql-codegen-esm --config ./codegen.ts",
    "codegen:wagmi": "wagmi generate",
    "sync-contracts": "./scripts/sync-contracts.sh",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.1",
    "graphql-tag": "^2.12.6",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "react": "18.3.0-canary-c3048aab4-20240326",
    "react-dom": "18.3.0-canary-c3048aab4-20240326",
    "viem": "2.41.2",
    "wagmi": "2.14.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@tanstack/react-query": "5.45.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20.19.25",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@wagmi/cli": "^2.9.0",
    "concurrently": "^7.6.0",
    "dotenv": "^17.2.3",
    "jsdom": "^27.4.0",
    "react": "18.3.0-canary-c3048aab4-20240326",
    "react-dom": "18.3.0-canary-c3048aab4-20240326",
    "tiny-invariant": "^1.3.1",
    "tsdown": "^0.9.0",
    "typescript": "5.1.6",
    "viem": "^2.41.2",
    "viem-deal": "^2.0.4",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5",
    "wagmi": "^2.14.11"
  }
}
